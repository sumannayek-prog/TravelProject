<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('./common/head') %>
</head>

<body>

  <!-- Spinner Start -->

<%- include('./common/spinner')%>

  <!-- Spinner End -->

  <!-- Topbar Start -->

<%- include('./common/topbar')%>

  <!-- Topbar End -->

  <!-- Navbar & Hero Start -->
  <div class="container-fluid position-relative p-0">
  <%- include('./common/navbar') %>
    </div>
  <!-- Navbar & Hero End -->

 <!-- Header Start -->
 <div class="container-fluid bg-breadcrumb">
  <div class="container text-center py-5" style="max-width: 900px;">
      <h3 class="text-white display-3 mb-4"><%= data.name %> : Dashboard</h1>
        <% if (error_msg.length>0) {%>
          <div class="alert alert-danger"><%= error_msg %></div>
        <%}%>
        <% if (success_msg.length>0) {%>
          <div class="alert alert-danger"><%= success_msg %></div>
        <%}%>
      <ol class="breadcrumb justify-content-center mb-0">
      </ol>    
  </div>
</div>
<!-- Header End -->

<!-- 404 Start -->
<div class="container-fluid py-5" style="background: linear-gradient(rgba(19, 53, 123, 0.3), rgba(19, 53, 153, 0.3)); object-fit: cover;">

          <div class="container">
          <table class="table table-bordered" style="color: black;">
            <thead>
                <tr>
                <th style="text-align: center; align-items: center;" scope="col">Id</th>
                      <th>Name</th>
                      <th>Email</th>
                      <th>City</th>
                      <th>Country</th>
                      <th>Guide</th>
                      <th>Hotel</th>
                      <th>Days</th>
                      <th>Adult</th>
                      <th>Kid</th>
                      <th>Amount</th>
                      <th>Food</th>
                      <th>Coupon Code</th>
                <th scope="col">Action</th>
              </tr>
            </thead>  
            <% let id = 0 
            if (allBookingdata.length > 0) {%>         
            <tbody>
              <% for (let item of allBookingdata)  {%>          
                <tr>
                <th style="align-items: center; text-align: center;" scope="row"><%= ++id %></th>
                      <td><%= item.name %></td>
                      <td><%= item.email %></td>            
                      <td><%= item.city %></td>
                      <td><%= item.country %></td>
                      <td><%= item.guide %></td>
                      <td><%= item.hotel %></td>
                      <td><%= item.days %></td>
                      <td><%= item.adult %></td>
                      <td><%= item.kid %></td>
                      <td><%= item.amount %></td>
                      <td><%= item.food %></td>
                      <td><%= item.coupon %></td>
                <td style="align-items: center; text-align: center;">
                <a href="deletebooking<%= item._id %>" class="btn btn-danger" >Delete</a>
      
                  </td>
              </tr> 

              
              <%} } %> 

                </td>
              
              </table>
          </div>
  </div>
</div>
<!-- 404 End -->


    <!-- Footer Start -->
        
    <%- include('./common/footer') %>

    <!-- Footer End -->
    
    
    <script type="text/javascript">  
              
        
  
    </script>  
    

    </body>

    </html>